import com.becoder.entity.Employee;
import com.becoder.repository.EmpRepo;
@Service
public class EmpService {

	@Autowired
	private EmpRepo repo;
	
	
	public void addEmp(Employee emp) {
		
		repo.save(emp);
		
		
		
		
		
		
		
	}
	
	
	
	
	
	
	
	
	
	
	
}
